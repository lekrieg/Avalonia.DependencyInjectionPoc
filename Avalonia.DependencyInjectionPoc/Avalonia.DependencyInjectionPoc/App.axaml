<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Avalonia.DependencyInjectionPoc"
             x:Class="Avalonia.DependencyInjectionPoc.App"
             RequestedThemeVariant="Dark">
  <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

  <Application.DataTemplates>
    <local:ViewLocator/>
  </Application.DataTemplates>

  <Application.Styles>
    <FluentTheme />
    <StyleInclude Source="avares://Avalonia.DependencyInjectionPoc/Assets/Icons.axaml"/>
    <StyleInclude Source="avares://Avalonia.DependencyInjectionPoc/StyledComponents/FloatingButtonBar.axaml"/>
  </Application.Styles>
</Application>